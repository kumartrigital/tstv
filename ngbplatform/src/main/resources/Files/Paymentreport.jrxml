	<queryString>
		<![CDATA[SELECT
    p.id AS paymentId,
    p.payment_date AS paymentDate,
    p.amount_paid as totalPayAmount,
    mcv.code_value as payMode,
    concat('Payment receied through','-',TRIM('Payment' from mcv.code_value),'-',DATE_FORMAT(p.payment_date,'%d-%M-%y')) as Description,
    mc.id as clientId,
    mc.account_no as accountNo,
    mc.display_name as displayName,
    mc.external_id as nationalId,
    cd.address_no as addressNo,
    cd.city AS city,
    cd.state AS state,
    cd.country AS country,
    cd.zip AS zip
FROM
    b_payments p
    JOIN m_client mc ON (mc.id=p.client_id and p.is_deleted=0)
    LEFT JOIN  b_client_address cd  ON (mc.id=cd.client_id and cd.address_key ='PRIMARY')
    LEFT JOIN  m_code_value mcv ON p.paymode_id = mcv.id
    WHERE p.id=$P{param1}]]>
	</queryString>
